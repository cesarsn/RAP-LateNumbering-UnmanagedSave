projection;
strict ( 2 );
use draft;
define behavior for ZC_Travel_U_CSN alias Travel
use etag
{
  use create;
  use update;
  use delete;

  use action Edit;
  use action Activate;
  use action Discard;
  use action Resume;
  use action Prepare;

  use function GetDefaultsForBookings;
  use association _Booking { create; with draft; }
}

define behavior for ZC_Booking_U_CSN alias Booking
use etag
{
  use update;
  use delete;

  use association _Travel { with draft; }
}